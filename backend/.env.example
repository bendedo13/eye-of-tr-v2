# Auth - Production'da ZORUNLU: openssl rand -hex 32
SECRET_KEY=your-secret-key-use-openssl-rand-hex-32

# Local geliştirme için
DEBUG=True

# Admin (prod'da mutlaka güçlü bir key kullanın)
# ADMIN_API_KEY=your-strong-admin-key

# Redis
# REDIS_URL=redis://redis:6379/0

# PostgreSQL örneği
# DATABASE_URL=postgresql+psycopg2://faceseek:faceseek@postgres:5432/faceseek

# JWT süresi (dakika, varsayılan 60 = 1 saat)
# ACCESS_TOKEN_EXPIRE_MINUTES=60

# Database (varsayılan SQLite, prod için Postgres önerilir)
# DATABASE_URL=sqlite:///./app.db
# DATABASE_URL=postgresql://user:pass@localhost/dbname

# CORS (virgülle ayır)
# CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# FAISS
# FAISS_DIR=faiss
# FAISS_INDEX_TYPE=flatl2
# FAISS_DIM=512
# FAISS_TOP_K_DEFAULT=3

# Face Embedding
# FACE_EMBEDDER_BACKEND=insightface
# INSIGHTFACE_MODEL=buffalo_l
# INSIGHTFACE_DET_THRESH=0.75
# INSIGHTFACE_DET_SIZE_W=640
# INSIGHTFACE_DET_SIZE_H=640

# FaceCheck
# FACECHECK_ENABLED=true
# FACECHECK_API_KEY=your-facecheck-key
# FACECHECK_API_URL=https://facecheck.id/api/v1
# FACECHECK_RATE_LIMIT_PER_MINUTE=30

# Yandex (XML Search)
# YANDEX_API_KEY=your-yandex-api-key
# YANDEX_FOLDER_ID=your-yandex-folder-id

# Bing
# BING_API_KEY=your-bing-api-key

# SerpAPI (Google Lens)
# SERPAPI_API_KEY=your-serpapi-key
# SERPAPI_ENGINE=google_lens
# SERPAPI_GL=tr
# SERPAPI_HL=tr
# SERPAPI_TIMEOUT=30

# RapidAPI (Real Time Image Search)
# RAPIDAPI_KEY=your-rapidapi-key
# RAPIDAPI_HOST=real-time-image-search.p.rapidapi.com
# RAPIDAPI_IMAGE_SEARCH_ENDPOINT=https://real-time-image-search.p.rapidapi.com/search
# RAPIDAPI_IMAGE_SEARCH_SIZE=any
# RAPIDAPI_IMAGE_SEARCH_COLOR=any
# RAPIDAPI_IMAGE_SEARCH_TYPE=any
# RAPIDAPI_IMAGE_SEARCH_TIME=any
# RAPIDAPI_IMAGE_SEARCH_USAGE_RIGHTS=any
# RAPIDAPI_IMAGE_SEARCH_FILE_TYPE=any
# RAPIDAPI_IMAGE_SEARCH_ASPECT_RATIO=any
# RAPIDAPI_IMAGE_SEARCH_SAFE_SEARCH=off
# RAPIDAPI_IMAGE_SEARCH_REGION=us

# Public base URL (SerpAPI gibi dış servislerin /uploads içeriğinize erişmesi için)
# PUBLIC_BASE_URL=https://www.face-seek.com
